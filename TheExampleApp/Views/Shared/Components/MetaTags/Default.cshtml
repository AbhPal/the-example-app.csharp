@model string
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content="@Localizer["metaDescription"]" />
<meta name="twitter:card" content="@Localizer["metaTwitterCard"]" />
<meta name="og:title" content="@Model" />
<meta name="og:type" content="article" />
<meta name="og:url" content="http://contentful-example-app.herokuapp.com" />
<meta name="og:image" content="http://contentful-example-app.herokuapp.com/og-image.jpg" />
<meta name="og:image:type" content="image/jpeg" />
<meta name="og:image:width" content="1200" />
<meta name="og:image:height" content="1200" />
<meta name="og:image:alt" content="@Localizer["metaImageAlt"]" />
<meta name="og:description" content="@Localizer["metaImageDescription"]" />
<link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon.png" />
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
<link rel="manifest" href="/manifest.json" />
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#4a90e2" />
<meta name="theme-color" content="#ffffff" />

<title>@Model</title>